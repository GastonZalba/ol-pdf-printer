{
  "name": "ol-pdf-printer",
  "version": "1.0.9",
  "description": "PDF Printer for Open Layers",
  "browser": "dist/ol-pdf-printer.min.js",
  "main": "dist/ol-pdf-printer.js",
  "module": "lib/ol-pdf-printer.js",
  "types": "lib/ol-pdf-printer.d.ts",
  "style": "lib/ol-pdf-printer.css",
  "scripts": {
    "precleanstart": "del-cli dist lib tmp-dist tmp-lib",
    "doc": "documentation readme src/ol-pdf-printer.ts --shallow --quiet=true --access=public --access=undefined --section=API --parse-extension ts && node scripts/external-links.js README.md && prettier --write README.md",
    "format": "eslint --fix src/*.ts && prettier --write src",
    "lint": "eslint src/*.ts && prettier --check src",
    "copy-assets": "ncp src/assets/ tmp-dist/assets/ && ncp src/assets/ tmp-lib/assets/",
    "build": "npm run lint && npm run precleanstart && tsc --pretty --project ./tsconfig.json && tsc --pretty --project ./tsconfig-dist.json && npm run copy-assets && rollup --config rollup.config.dist.js && rollup --config rollup.config.js",
    "postbuild": "ncp tmp-lib/ol-pdf-printer.d.ts dist/ol-pdf-printer.d.ts && ncp tmp-lib/ol-pdf-printer.d.ts lib/ol-pdf-printer.d.ts && ncp src/assets/css/bootstrap.min.css dist/css/bootstrap.min.css && ncp src/assets/css/bootstrap.min.css lib/css/bootstrap.min.css && del-cli tmp-dist tmp-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GastonZalba/ol-pdf-printer.git"
  },
  "keywords": [
    "ol",
    "openlayers",
    "pdf",
    "print",
    "gis",
    "sig",
    "wms"
  ],
  "author": "GastÃ³n Zalba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GastonZalba/ol-pdf-printer/issues"
  },
  "homepage": "https://github.com/GastonZalba/ol-pdf-printer#readme",
  "peerDependencies": {
    "ol": ">=5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-react-jsx": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/node": "^14.14.10",
    "@types/ol": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "clean-css": "^4.2.3",
    "del-cli": "^3.0.1",
    "documentation": "^13.1.0",
    "eslint-config-openlayers": "^14.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ncp": "^2.0.0",
    "ol": "^6.5.0",
    "prettier": "^2.2.1",
    "rollup": "^2.33.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "dom-to-image-improved": "^2.8.0",
    "jspdf": "^2.3.1",
    "locale-enum": "^1.1.1",
    "modal-vanilla": "^0.8.0",
    "pdfjs-dist": "^2.9.359"
  }
}
